{
  "expo": {
    "name": "NeuraChat",
    "slug": "NeuraChat",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/NeuraChat1.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/NeuraChat1.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.abdirashiidbx.NeuraChat"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/NeuraChat1.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/NeuraChat1.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "eas": {
        "projectId": "ed7d34cb-09cd-4ea2-a231-cce7541f82e7"
      }
    }
  }
}
